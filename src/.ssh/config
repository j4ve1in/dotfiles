Host ytet5uy4
  HostName ssh.ytet5uy4.com
  User core
  Port 2022

Host ytet5uy4-vault
  HostName ssh.vault.ytet5uy4.com
  User core
  Port 2022

Include private/*

Host *
  PKCS11Provider /usr/lib/kr-pkcs11.so
  ProxyCommand /usr/bin/krssh %h %p

ControlMaster auto
ControlPersist no
ControlPath ~/.ssh/sockets/socket-%r@%h:%p
