private -a selector_widgets
selector_widgets=(
  `ls .zsh/autoload/selector/ | \
    sed -e 's/selector-//g' -e 's/-widget//g' | \
    egrep -v '^select$'`
)
private widget
widget=$(echo ${(F)selector_widgets} | $SELECTOR)
[[ -z $widget ]] && return 1
selector-${widget}-widget

