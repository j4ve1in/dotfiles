! urxvt
!! Base
URxvt.title: urxvt
URxvt.termName: rxvt-unicode-256color
URxvt.saveLines: 20000
URxvt.scrollBar: false
URxvt.matcher.button: 1
URxvt.url-launcher: chromium
URxvt.searchable-scrollback: false
!! perl-ext
URxvt.perl-ext-common: default,matcher,fullscreen,resize-font,selection,eval
!!! fullscreen
URxvt.keysym.F11: perl:fullscreen:switch
!!! eval
URxvt.keysym.C-S-C:  eval:selection_to_clipboard
URxvt.keysym.C-S-V:  eval:paste_clipboard

!! iso
URxvt.iso14755: false
URxvt.iso14755_52: false

!! Color
URxvt.depth: 32
URxvt.foreground: rgba:ffff/ffff/ffff/ffff
URxvt.background: rgba:0000/0000/0000/cccc
URxvt.cursorColor: blue
URxvt.cursorColor2: white

!! Font
URxvt.font: xft:Migu 1M:size=13:antialias=true:hinting=true, \
            xft:InconsolataForPowerline Nerd Font:size=13:antialias=true:hinting=true \
            xft:DejaVu Sans:size=13:antialias=true:hinting=true
URxvt.boldFont: xft:Migu 1M:bold:size=13:antialias=true:hinting=true, \
                xft:InconsolataForPowerline Nerd Font:bold:size=13:antialias=true:hinting=true \
                xft:DejaVu Sans:bold:size=13:antialias=true:hinting=true
